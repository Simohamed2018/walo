package com.simo.model;

public class Compte {
     
    private int id;
    
    private String nomDetenteur;
    
    private String banque;      
   
    private String typeCompte;    
   
    private String descriptionCompte;
    private Double solde;

	public Compte() {
		
	}

	public Compte(int id, String nomDetenteur, String banque, String typeCompte, String descriptionCompte) {
		super();
		this.id = id;
		this.nomDetenteur = nomDetenteur;
		this.banque = banque;
		this.typeCompte = typeCompte;
		this.descriptionCompte = descriptionCompte;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getNomDetenteur() {
		return nomDetenteur;
	}

	public void setNomDetenteur(String nomDetenteur) {
		this.nomDetenteur = nomDetenteur;
	}

	public String getBanque() {
		return banque;
	}

	public void setBanque(String banque) {
		this.banque = banque;
	}

	public String getTypeCompte() {
		return typeCompte;
	}

	public void setTypeCompte(String typeCompte) {
		this.typeCompte = typeCompte;
	}

	public String getDescriptionCompte() {
		return descriptionCompte;
	}

	public void setDescriptionCompte(String descriptionCompte) {
		this.descriptionCompte = descriptionCompte;
	}

	@Override
	public String toString() {
		return "Compte [id=" + id + ", nomDetenteur=" + nomDetenteur + ", banque=" + banque + ", typeCompte="
				+ typeCompte + ", descriptionCompte=" + descriptionCompte + "]";
	}
	
    
    
}
    
   
   
   