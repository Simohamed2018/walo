package com.mproduits.service;

import com.mproduits.model.Compte;
import org.springframework.stereotype.Service;

import java.util.List;
//@Service
public interface CompteService {


    List<Compte> listeDesProduits();

    Compte recupererUnProduit(int id);

    Compte ajouterUnProduit(Compte produit);

    void supprimerProduit(int id);    

    Compte ajouterProduit(Compte product);

    List<Compte> calculerMargeProduit();
}
